[module/sep2]
type = custom/text
content = "|"
content-foreground = #93A1A1
content-background = ${colors.background-alt}

; ---------------------------------------------------------

[module/weather]
type = custom/script
exec = "/home/gel0/bashscripts/weather.sh"
interval = 600
label-font = 5
;label-active-font = 6
;label = %{T1} %output% %{T-}

